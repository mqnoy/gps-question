  "scripts": {
    "app:setup": "yarn sequelize db:create && yarn sequelize db:migrate",
    "app:reset": "yarn sequelize db:drop",
    "db:sync": "yarn dotenv -e .env yarn sequelize db:migrate",
    "build": "yarn tsc",
    "dev": "yarn dotenv -e .env  yarn ts-node src/index.ts --transpile-onl",
    "start": "vercel dev",
    "clean": "rm -rf dist",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "luxon": "^3.4.4",
    "pg": "^8.11.5",
    "randomstring": "^1.3.0",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/luxon": "^3.4.2",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "dotenv-cli": "^7.4.2",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "sequelize-cli": "^6.6.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
